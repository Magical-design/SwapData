<Window x:Class="SwapData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SwapData"
        mc:Ignorable="d"
        Title="SwapData 1.0114" Height="450" Width="620">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary1.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TabControl SelectedItem="{Binding ElementName=TabItem1, Mode=OneWay}"  >
            <TabItem x:Name="TabItem1" Header="MAIN" >
                <Grid >
                    <local:MainView  />
                </Grid>
            </TabItem>

        </TabControl>
    </Grid>
</Window>
